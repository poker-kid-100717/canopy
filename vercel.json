{
    "name": "canopy",
    "builds": [
      { "src": "backend/api/index.py", "use": "@vercel/python" },
      { "src": "frontend/package.json", "use": "@vercel/static-build" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "backend/api/index.py" },
      { "src": "/(.*)", "dest": "frontend/build/$1" }
    ]
  }